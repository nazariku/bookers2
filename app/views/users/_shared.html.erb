<html><!--左上テンプレート-->
  <body>
    <h2>User info</h2>

      <%= image_tag user.get_profile_image(100, 100) %>
      
    <div>
     

    <table>
      <tbody>
        <tr>
          <th>name</th>
          <th><%= user.name %></th>
        </tr>
        <tr>
          <th>introduction</th>
          <th><%#= user.introduction %></th>
        </tr>
      </tbody>
    </table>
    <div><%= link_to "編集", edit_user_path(current_user) %></div>

    </div>
    
     <h1>New book</h1>
      <%= form_with model: Book.new do |f| %>
        <h4>title</h4>
          <%= f.text_field :title %>
        <h4>body</h4>
          <%= f.text_area :body %>
        <%= f.submit '投稿' %>
      <% end %>
  </body>
</html>